unit UBoard;

interface
uses SysUtils, Generics.Collections, Generics.Defaults,USquare;
type TBoard=class
const
  SIZE:integer(40);
  private
    squares:TList<SIZE>;
  public
    function Board;
    procedure getSquare(start:TSquare; distance:integer);
    function getStartSquare:TSquare;
end;

implementation

end.
